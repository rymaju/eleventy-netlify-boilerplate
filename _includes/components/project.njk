<article{% if post.url == url %} data-current="current item"{% endif %}>
			<h3 class="summary-title">
				<a href="{{ project.url | url }}">
					{% if project.title %}
						{{ project.title }}
					{% else %}
						Untitled
					{% endif %}
				</a>
			</h3>
			{% if project.summary %}
				<p class="subtitle">
					{{ project.summary }}
				</p>
			{% endif %}
		</article>